{{=<% %>=}}
<div>
    <core-course-module-description description="<% autoattendmod.intro %>" component="mod_autoattendmod" componentId="<% cmid %>"></core-course-module-description>
 
    <ion-list>
        <ion-list-header>
            <p class="item-heading">{{ 'plugin.mod_autoattendmod.summaryofattempts' | translate }}</p>
        </ion-list-header>
 
        <%#issues%>
            <ion-item>
                <button ion-button block color="light" core-site-plugins-new-content title="<% autoattendmod.name %>" component="mod_autoattendmod" method="mobile_issues_view" [args]="{cmid: <% cmid %>, courseid: <% courseid %>}">
                    {{ 'plugin.mod_autoattendmod.viewautoattendmodviews' | translate: {$a: <% numissues %>} }}
                </button>
            </ion-item>
        <%/issues%>
 
        <%#showget%>
        <ion-item>
            <button ion-button block core-course-download-module-main-file moduleId="<% cmid %>" courseId="<% autoattendmod.course %>" component="mod_autoattendmod" [files]="[{fileurl: '<% issue.fileurl %>', filename: '<% issue.filename %>', timemodified: '<% issue.timemodified %>', mimetype: '<% issue.mimetype %>'}]">
                <ion-icon name="cloud-download" item-start></ion-icon>
                {{ 'plugin.mod_autoattendmod.getautoattendmod' | translate }}
            </button>
        </ion-item>
        <%/showget%>
 
        <%^showget%>
        <ion-item>
            <p>{{ 'plugin.mod_autoattendmod.requiredtimenotmet' | translate }}</p>
        </ion-item>
        <%/showget%>
 
        <!-- Call log WS when the template is loaded. -->
        <span core-site-plugins-call-ws-on-load name="mod_autoattendmod_view_autoattendmod" [params]="{autoattendmodid: <% autoattendmod.id %>}" [preSets]="{getFromCache: 0, saveToCache: 0}"></span>
    </ion-list>
</div>

